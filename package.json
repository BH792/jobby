{
  "name": "jobby",
  "version": "0.1.0",
  "description": "Job Search Organizer",
  "private": true,
  "engines": {
    "node": "8.x.x"
  },
  "scripts": {
    "start": "node server",
    "heroku-postbuild": "cd client/ && npm install && npm install --only=dev --no-shrinkwrap && npm run build",
    "start:dev": "DEBUG=slack-checkin-app:* nodemon --exec 'node -r dotenv/config' ./bin/www",
    "db:migrate:seed": "sequelize db:migrate && sequelize db:seed:all",
    "db:reset": "sequelize db:drop && sequelize db:create && sequelize db:migrate && sequelize db:seed:all"
  },
  "cacheDirectories": [
    "node_modules",
    "client/node_modules"
  ],
  "dependencies": {
    "bcrypt": "^1.0.3",
    "body-parser": "~1.17.1",
    "debug": "~2.6.3",
    "dotenv": "^4.0.0",
    "express": "~4.15.2",
    "jsonwebtoken": "^8.1.0",
    "morgan": "~1.8.1",
    "nodemon": "^1.12.1",
    "passport": "^0.4.0",
    "pg": "<7.0.0",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.20.1",
    "sequelize-cli": "^3.0.0",
    "serve-favicon": "~2.4.2"
  }
}
